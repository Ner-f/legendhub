<!doctype html>
<html lang="en">
    <head>
        <%-include("../shared/meta")-%>
    </head>
    <body ng-app="legendwiki-app">
        <%-include("../shared/header")-%>
        <div class="container">
            <div class="row">
                <h1 class="text-primary"><%=title%></h1>
            </div>
            <div class="row justify-content-center justify-content-md-start">
                <div class="btn-group btn-block" role="group" aria-label="Toolbar">
                    <button type="button" class="btn btn-outline-primary"><%=new Date(vm.changelog.createdOn).toISOString().slice(0,16).replace("T"," ")%> UTC</button>
                    <a class="btn btn-outline-primary" role="button" href="<%-(!locals.user ? "/login.html?returnUrl=" : "")%>/changelog/edit.html?id=<%-vm.changelog.id%>"><i class="far fa-edit"></i><span class="d-none d-md-inline"> Edit</span></a>
                </div>
            </div>
        </div>
        <%-include("../shared/footer")-%>
        <%-include("../shared/scripts")-%>
    </body>
</html>
